  "author": "Evan Turner <ev@evturn.com>",
  "scripts": {
    "start": "babel-node index.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-preset-es2015": "^6.13.2",
    "exif": "^0.6.0",
    "shelljs": "^0.7.4"
  },
  "keywords": [
    "exif",
    "osx",
    "macos",
    "metadata",
    "photos",
    "images",
